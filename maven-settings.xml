<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <!-- Refer to https://wiki.vip.corp.ebay.com/display/scmteam/FAQ+About+eBayCentral+Migration+To+Artifactory+on+Tess -->
    <servers>
        <server>
            <id>central</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
        <server>
            <id>releases</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
        <server>
            <id>snapshots</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
        <server>
            <id>thirdparty</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
        <server>
            <id>commercial</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
        <server>
            <id>plugins</id>
            <username>RaptorTeam</username>
            <password>RaptorTeam</password>
        </server>
    </servers>

    <mirrors>
        <mirror>
            <id>central</id>
            <mirrorOf>central</mirrorOf>
            <url>https://ebaycentral.qa.ebay.com/content/repositories/central</url>
        </mirror>
    </mirrors>

    <profiles>
        <profile>
            <id>ebay</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/central</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>releases</id>
                    <url>https://ebaycentral.qa.ebay.com/content/repositories/releases/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>snapshots</id>
                    <url>https://ebaycentral.qa.ebay.com/content/repositories/snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>thirdparty</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/thirdparty</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>commercial</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/commercial</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/central</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>releases</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/releases</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>plugins</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/plugins</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>snapshots</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>thirdparty</id>
                    <url>https://ebaycentral.corp.ebay.com/content/repositories/thirdparty</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>ebay</activeProfile>
    </activeProfiles>

</settings>
